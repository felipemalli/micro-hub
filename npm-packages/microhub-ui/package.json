{
  "name": "@felipemalli-libs/microhub-ui",
  "version": "2.1.18",
  "description": "Web Components design system with Stencil",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2022": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/microhub-ui/microhub-ui.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./react": {
      "import": "./react-dist/index.js",
      "types": "./react-dist/index.d.ts"
    },
    "./styles.css": "./dist/microhub-ui/microhub-ui.css"
  },
  "files": [
    "dist/",
    "loader/",
    "react/",
    "react-dist/"
  ],
  "scripts": {
    "build": "pnpm run build:web-components && pnpm run build:react",
    "build:web-components": "stencil build",
    "build:react": "tsc -p tsconfig.react.json",
    "postbuild": "pnpm run build:react",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "deploy": "pnpm publish --access public"
  },
  "dependencies": {
    "@stencil/react-output-target": "^1.2.0"
  },
  "devDependencies": {
    "@stencil/core": "^4.36.2",
    "@types/jest": "^29.5.5",
    "@types/react": "^19.1.10",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "react": "^19.1.1",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "microfrontends",
    "typescript",
    "web-components"
  ],
  "author": "Felipe Vahia Malliagros",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "pnpm": ">=10.14.0"
  }
}
